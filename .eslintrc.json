{
  "parser": "babel-eslint",
  "extends": ["airbnb"],
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "no-console": "off",
    "comma-dangle": "off",
    "react/jsx-filename-extension": "off",
    "prefer-promise-reject-errors": "off",
    "no-param-reassign": "off",
    "no-underscore-dangle": "off",
    "linebreak-style": ["error", "windows"]
  }
}
